<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.android.app.buystoreapp.managementservice.ExplainWebViewActivity">
    <RelativeLayout
        android:id="@+id/rl_explain_title"
        android:layout_width="match_parent"
        android:layout_alignParentTop="true"
        android:layout_height="45dp"
        android:background="#ffffff">

        <TextView
            android:id="@+id/tv_explain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:gravity="center"
            android:layout_marginRight="68dp"
            android:text="提现说明"
            android:textColor="#404040"
            android:textSize="19sp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/ib_explain_back"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:id="@+id/ib_explain_back"
            android:layout_width="68dp"
            android:layout_height="45dp"
            android:layout_centerVertical="true"
            android:scaleType="centerInside"
            android:layout_alignParentLeft="true"
            android:background="@null"
            android:src="@drawable/ic_arrows_address_left" />
    </RelativeLayout>
    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>
